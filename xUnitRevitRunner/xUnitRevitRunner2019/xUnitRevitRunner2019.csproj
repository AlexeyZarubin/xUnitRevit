    <ProjectReference Include="..\..\xUnitRevitUtils\xUnitRevitUtils2023\xUnitRevitUtils2023.csproj">
      <Project>{637136c9-ab5d-4f00-948e-c68fae2682d0}</Project>
      <Name>xUnitRevitUtils2023</Name>
